@{
    ViewBag.Title = "Home Page";
}
<script src="~/Scripts/angular.js"></script>
<div ng-app="Homeapp" ng-controller="HomeController">
    <div class="col-md-12">
        <br />
        <br />
        <label >Nome</label>
        <input type="text" ng-model="register.nome" class="form-control"/>
        <label >Sobrenome</label>
        <input type="text" ng-model="register.sobrenome" class="form-control"/>
        <label >Idade</label>
        <input type="text" ng-model="register.idade"class="form-control"/>
    <br />
<input type="button"class="btn btn-primary" ng-click="savedata()" value="{{btntext}}"/>
    <input type="button" class="btn btn-warning" ng-click="MudaPagina()" value="Ver Dados"/>
    </div>
</div>
<script src="~/Scripts/home.js"></script>